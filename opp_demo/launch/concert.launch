<launch>
    <include file="$(find concert_master)/launch/concert_master.launch">
        <arg name="concert_name" value="OPP Concert"/>
    </include>
    <group ns="concert">
        <node pkg="concert_service_manager" name="add_service" type="add_service_from_file.py">
            <param name="filename" value="$(find concert_service)/services/turtle.service"/>
        </node>

        <node pkg="concert_service_manager" name="add_service2" type="add_service_from_file.py">
            <param name="filename" value="$(find concert_service)/services/chatter.service"/>
        </node>
        
      	<node pkg="concert_service_manager" name="add_service3" type="add_service_from_file.py">
            <param name="filename" value="$(find opp_demo)/services/teleop.service"/>
        </node>

        <node pkg="concert_service_manager" name="add_service4" type="add_service_from_file.py">
            <param name="filename" value="$(find opp_demo)/services/cafe_delivery.service"/>
        </node>
    </group>
</launch>
