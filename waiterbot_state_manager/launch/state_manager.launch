<launch>
  <!-- state manager -->
  <node pkg="waiterbot_state_manager" name="waiterbot_state_manager" type="state_manager_node.py">
    <remap from="android_ui/drink_order" to="drink_order"/>
    <remap from="android_ui/drink_ar" to="drink_ar"/>
    <remap from="android_ui/drinks_dispensed" to="drinks_dispensed"/>
    <remap from="android_ui/tray_empty" to="tray_empty"/>
    <remap from="waiterbot_nav_control/goto" to="goto"/>
    <remap from="waiterbot_nav_control/status" to="status"/>
    <remap from="waiterbot_state_manager/debug" to="waiterbot_debug"/>
  </node>

  <!-- dummy vm feedback -->
  <node pkg="waiterbot_state_manager" name="dummy_vm_feedback" type="dummy_vm_feedback.py"/>

</launch>
